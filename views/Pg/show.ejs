<main>

  <section class="py-5 text-center container">
    <div class="row py-lg-5">
      <div class="col-lg-6 col-md-8 mx-auto">
        <h1 class="fw-light">Get My Pg <sub>easy</sub></h1>
        <p class="lead text-muted">Easy Compare & Book in one go.</p>
        <p>
          <a href="#" class="btn btn-primary my-2">Main call to action</a>
          <a href="#" class="btn btn-secondary my-2">Secondary action</a>
        </p>
      </div>
    </div>
  </section>

  <div class="album py-5 bg-light">
    <div class="container">

      <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">

        <% for(let pg of Pgs ) { %> 
          <div class="col">
            <div class="card shadow-sm">
              <img style="width: 100%;height: 15vw;object-fit: cover;" src="<%= pg.image[0].url %> " alt="" srcset="">
              <!-- <svg class="bd-placeholder-img card-img-top" width="100%" height="225" xmlns="<%= pg.image %>" role="img" aria-label="Placeholder: Thumbnail" preserveAspectRatio="xMidYMid slice" focusable="false"><title>Placeholder</title><rect width="100%" height="100%" fill="#55595c"/><text x="50%" y="50%" fill="#eceeef" dy=".3em">Thumbnail</text></svg> -->
              <div class="card-body">
                <p class="card-text"><%= pg.description %> </p>
                <div class="d-flex justify-content-between align-items-center">
                  <div class="btn-group">
                    <!-- <button type="button" class="btn btn-sm btn-outline-secondary" href=/home/<%= pg._id %> ">View</button> -->
                    <a type="button" class="btn btn-sm btn-outline-success" href="/home/<%= pg._id %> ">View</a>
                    <a type="button" class="btn btn-sm btn-outline-info" href="/home/<%= pg._id %>/edit ">Edit</a>
                    <!-- <button type="button" class="btn btn-sm btn-outline-secondary">Edit</button> -->
                  </div>
                  <small class="text-muted"><strong>Available</strong></small>
                </div>
              </div>
            </div>
          </div>
          <% } %> 
      </div>
    </div>
  </div>

</main>

<footer class="text-muted py-5">
