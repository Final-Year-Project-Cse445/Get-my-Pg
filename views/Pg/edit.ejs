<div class="container1">
    <form class="row g-3" action="/home/<%= Pg._id %>?_method=PUT " method="post" enctype="multipart/form-data">
   <div class="col-md-6">
      <label for="title">Title: </label>
      <input type="text" id="title" name="pg[title]" placeholder="" value="<%= Pg.title %>" required>
   </div>
   <div class="col-md-6">
      <label for="price">Price:</label>
      <input type="text" id="price" name="pg[price]" placeholder="" value="<%= Pg.price %>" required>
   </div>
   
   <div class="col-md-6">
      <label for="location">Location: </label>
      <input type="text" id="location" name="pg[location]" placeholder="" value="<%= Pg.location %>" required>
   </div>

   <div class="col-md-6">
      <label for="rating">Rating: </label>
      <input type="text" id="rating" name="pg[rating]" placeholder="" value="<%= Pg.rating %>" required>
   </div>

   <div class="col-md-6">
    <label for="image">Image: </label>
    <input type="file" id="image" name="pg[image]" placeholder=""  multiple >
  </div>

   <div class="col-md-6">
    <label for="description">Description: </label>
    <input type="text" id="description" name="pg[description]" placeholder="" value="<%= Pg.description %>" required>
 </div>
  <div class="col-md-3">
    <% Pg.image.forEach(function(img,i) { %>
      <img src="<%= img.url %> "  class="img-fluid img-thumbnail" alt="" srcset=""> 
      <div class="form-check-inline">
          <input type="checkbox" id="image-<%= i %>" name="deleteImages[]" value="<%= img.filename %>" >
      </div>
     <div class="col-md-6">
      <label for="image-<%= i %> ">Delete ?</label>
      <% }) %>
      </div>
  <div class="col-12">
  <button type="submit" class="btn btn-primary">Add</button>
  </div>
 
</form>
</div>



