    <section class="section-sm">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="search-result bg-gray">
              <h2>Update Pgs</h2>
              <p>Select One to Update</p>
            </div>
          </div>
        </div>
        <div class="row">
          
          <div class="col-md-12">
            <div class="product-grid-list">
              <div class="row mt-30">
                <% for(let pg of Pgs ) { %> 
                <div class="col-sm-12 col-lg-3 col-md-6">
                  <!-- product card -->
                  <div class="product-item bg-light">
                    <div class="card">
                      <div class="thumb-content">
                        <!-- <div class="price">$200</div> -->
                        <a href="/home/<%= pg._id %>">
                          <img
                            class="card-img-top img-fluid"
                            style="width: 100%; height: 15vw; object-fit: cover"
                            src="<%= pg.image[0].url %>"
                            alt="Card image cap"
                          />
                        </a>
                      </div>
                      <div class="card-body">
                        <h4 class="card-title">
                          <a href="/home/<%= pg._id %>"><%= pg.title %></a>
                        </h4>
                        <p class="card-text"><%= pg.location %></p>
                        <a href="/home/<%=pg._id %>/edit" id="edit" class="btn  btn-sm mt-1 active bg-info text-white border-0 rounded font-weight-bold" role="button">Update</a>
                      </div>
                    </div>
                  </div>
                </div>
                <% } %>    
              </div>
            </div>
            
          </div>
        </div>
      </div>
    </section>
